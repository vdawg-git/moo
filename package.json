		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.4",
		"zod-validation-error": "^3.4.0"
	},
	"devDependencies": {
		"dotenv-cli": "^8.0.0",
		"@biomejs/biome": "2.2.5",
		"@types/bun": "1.2.23",
		"@types/node": "24.6.2",
