{
	"name": "moo",
	"version": "0.2.0",
	"license": "MIT",
	"type": "module",
	"main": "source/index.ts",
	"module": "source/index.ts",
	"scripts": {
		"dev": "DEV=true bun --watch .",
		"debug": "DEV=true bun --inspect-wait=1111/a .",
		"debug-prod": "NODE_ENV=production bun --inspect-wait=1111/a .",
		"prod": "NODE_ENV=production bun .",
		"build": "echo we need no build",
		"writeSchemas": "bun run ./scripts/writeSchemas.ts",
		"compile": "bun build --compile  ./source/index.ts --no-compile-autoload-dotenv --sourcemap ./source/index.ts --outfile ./dist/moo",
		"release": "dotenv -- release-it",
		"types": "tsc",
		"lint": "oxlint --type-aware",
		"lint:fix": "oxlint --type-aware --fix",
		"db:generate": "rm -rf ./drizzle/* && drizzle-kit generate && rm -r ./drizzle/meta && cd ./drizzle && mv * setup.sql",
		"drizzleStudio": "DEV=true bunx drizzle-kit studio",
		"format": "prettier . --write"
	},
	"dependencies": {
		"@drizzle-team/brocli": "0.11.0",
		"@jellybrick/mpris-service": "2.1.5",
		"@libsql/client": "0.15.15",
		"@opentui/core": "0.1.56",
		"@opentui/react": "0.1.56",
		"@xstate/store": "3.9.2",
		"any-date-parser": "2.2.3",
		"chokidar": "4.0.3",
		"cowsay": "1.6.0",
		"drizzle-orm": "0.44.6",
		"env-paths": "3.0.0",
		"fuse.js": "7.1.0",
		"json5": "2.2.3",
		"music-metadata": "11.9.0",
		"mutative": "1.2.0",
		"natural-orderby": "5.0.0",
		"oxlint": "1.38.0",
		"react": "19.1.1",
		"react-error-boundary": "5.0.0",
		"remeda": "2.32.0",
		"rxjs": "7.8.2",
		"ts-pattern": "5.8.0",
		"typescript-result": "3.1.1",
		"untildify": "5.0.0",
		"winston": "3.17.0",
		"yaml": "2.7.0",
		"zod": "4.1.11",
		"zod-validation-error": "4.0.2"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "4.7.0",
		"@prettier/plugin-oxc": "0.1.3",
		"@types/bun": "1.2.23",
		"@types/node": "24.6.2",
		"@types/react": "19.1.1",
		"dotenv-cli": "8.0.0",
		"drizzle-kit": "0.30.5",
		"oxlint-tsgolint": "0.11.0",
		"prettier": "3.7.4",
		"release-it": "19.2.3",
		"type-fest": "5.0.1",
		"typescript": "5.9.3"
	},
	"patchedDependencies": {
		"@jellybrick/mpris-service@2.1.5": "patches/@jellybrick%2Fmpris-service@2.1.5.patch",
		"@jellybrick/dbus-next@0.10.3": "patches/@jellybrick%2Fdbus-next@0.10.3.patch"
	},
	"private": true
}
