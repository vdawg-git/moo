		"any-date-parser": "^2.0.3",
		"chokidar": "^4.0.3",
		"cowsay": "^1.6.0",
		"drizzle-orm": "0.40.0",
		"env-paths": "^3.0.0",
		"fuse.js": "^7.1.0",
		"json5": "^2.2.3",
		"music-metadata": "^11.9.0",
		"mutative": "^1.2.0",
		"natural-orderby": "^5.0.0",
		"react": "18.2.0",
		"react-error-boundary": "^5.0.0",
		"remeda": "^2.32.0",
		"rxjs": "^7.8.2",
